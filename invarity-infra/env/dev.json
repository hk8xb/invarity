{
  "envName": "dev",
  "project": "invarity",
  "tags": {
    "Project": "invarity",
    "Env": "dev",
    "ManagedBy": "cdk"
  },
  "vpc": {
    "maxAzs": 2,
    "natGateways": 1
  },
  "ecs": {
    "cpu": 256,
    "memoryMiB": 512,
    "desiredCount": 0,
    "healthCheckPath": "/healthz",
    "containerPort": 8080
  },
  "logs": {
    "retentionDays": 14
  },
  "s3": {
    "manifestsPrefix": "manifests",
    "auditPrefix": "audit"
  },
  "identity": {
    "cognito": {
      "callbackUrls": ["http://localhost:3000/callback", "http://localhost:3000/auth/callback"],
      "logoutUrls": ["http://localhost:3000", "http://localhost:3000/logout"],
      "domainPrefix": "invarity-dev"
    }
  }
}
